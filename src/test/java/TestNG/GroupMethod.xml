<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">


<suite name="MySuite">
  <test name="MyTest">
	  <groups>
		  <run>
			  <include name="regression"></include>		<!-- It run regression group  -->
			  <exclude name="sanity"></exclude>			<!-- It Exlude the sanity group  -->
